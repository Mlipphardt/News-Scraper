<div class="container">

    <div class="row">
        <div class="col-lg-3"></div>
        <div class="col-lg-6 text-center">
            <h2>The Daily News</h2>
        </div>
        <div class="col-lg-3"></div>
    </div>


    <div class="row">
        <div class="col-lg-1"></div>
        <div class="col-lg-10">
            {{#each articles}}
            <div class="article-div">
                <a href={{this.link}}>{{this.title}}</a>
                <p>{{this.description}}</p>
            </div>
            {{/each}}
        </div>
        <div class="col-lg-1"></div>

    </div>

</div>